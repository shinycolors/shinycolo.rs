{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "types": ["vue-typescript-import-dts"]
  },
  "include": ["./src/**/*"]
}
